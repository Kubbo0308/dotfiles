{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "/Users/takesupasankyu/.local/bin/uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "claude-code",
        "--mode",
        "interactive",
        "--mode",
        "no-editing"
      ],
      "env": {}
    },
    "chrome-devtools": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "chrome-devtools-mcp@latest"
      ],
      "env": {}
    }
  }
}
